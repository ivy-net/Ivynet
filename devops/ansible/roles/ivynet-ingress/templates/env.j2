DATABASE_URL=postgresql://{{ ivynet_ingress_postgres_user}}:{{ ivynet_ingress_postgres_user_pass }}@{{ ivynet_ingress_postgres_host }}:5432/{{ ivynet_ingress_postgres_db }}
IVY_GRPC_TLS_CERT={{ ivynet_ingress_path_secrets }}/self.pem
IVY_GRPC_TLS_KEY={{ ivynet_ingress_path_secrets }}/self.key
{% if ivynet_ingress_logs_lowlevel %}IVY_LOG_LEVEL={{ ivynet_ingress_logs_lowlevel }}{% endif %}
{% if ivynet_ingress_notification_telegram|d('') | length > 0 %}TELEGRAM_TOKEN={{ ivynet_ingress_notification_telegram }}{% endif %}
{% if ivynet_ingress_sendgrid %}
IVY_ROOT_URL={{ ivynet_ingress_site_url }}
SENDGRID_KEY={{ ivynet_ingress_sendgrid_key }}
SENDGRID_FROM={{ ivynet_ingress_sengrid_from }}
STN_GENERIC={{ ivynet_ingress_sengrid_alert_generic }}
{% endif %}
