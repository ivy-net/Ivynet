DATABASE_URL=postgresql://{{ ivynet_backend_postgres_user}}:{{ ivynet_backend_postgres_user_pass }}@{{ ivynet_backend_postgres_host }}:5432/{{ ivynet_backend_postgres_db }}
IVY_GRPC_TLS_CERT={{ ivynet_backend_path_secrets }}/self.pem
IVY_GRPC_TLS_KEY={{ ivynet_backend_path_secrets }}/self.key
{% if ivynet_backend_logs_lowlevel %}IVY_LOG_LEVEL={{ ivynet_backend_logs_lowlevel }}{% endif %}
{% if ivynet_backend_sendgrid %}
IVY_ROOT_URL={{ ivynet_backend_site_url }}
SENDGRID_API_KEY={{ ivynet_backend_sendgrid_key }}
SENDGRID_ORG_VER_TMP={{ ivynet_backend_sendgrid_org }}
SENDGRID_USER_VER_TMP={{ ivynet_backend_sendgrid_user }}
SENDGRID_PASS_RESET_TMP={{ ivynet_backend_sendgrid_pass_reset }}
