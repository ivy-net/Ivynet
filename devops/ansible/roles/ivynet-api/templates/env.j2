DATABASE_URL=postgresql://{{ ivynet_api_postgres_user}}:{{ ivynet_api_postgres_user_pass }}@{{ ivynet_api_postgres_host }}:5432/{{ ivynet_api_postgres_db }}
IVY_GRPC_TLS_CERT={{ ivynet_api_path_secrets }}/self.pem
IVY_GRPC_TLS_KEY={{ ivynet_api_path_secrets }}/self.key
{% if ivynet_api_logs_lowlevel %}IVY_LOG_LEVEL={{ ivynet_api_logs_lowlevel }}{% endif %}
{% if ivynet_api_sendgrid %}
IVY_ROOT_URL={{ ivynet_api_site_url }}
SENDGRID_API_KEY={{ ivynet_api_sendgrid_key }}
SENDGRID_ORG_VER_TMP={{ ivynet_api_sendgrid_org }}
SENDGRID_USER_VER_TMP={{ ivynet_api_sendgrid_user }}
SENDGRID_PASS_RESET_TMP={{ ivynet_api_sendgrid_pass_reset }}
{% endif %}
