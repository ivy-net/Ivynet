  "query": "SELECT\n                a.user_id,\n                a.organization_id,\n                a.email,\n                a.password,\n                a.role AS \"role!: Role\",\n                a.created_at,\n                a.updated_at\n              FROM account a\n              JOIN organization o ON a.organization_id = o.organization_id\n              WHERE a.email = $1 AND o.verified = TRUE",
